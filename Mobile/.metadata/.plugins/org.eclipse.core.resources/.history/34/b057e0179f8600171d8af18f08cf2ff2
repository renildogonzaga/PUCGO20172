package ex001xml;

public class Dados {
	
	Object Universidade;
	Object Professor;
	Object Sala;
	Object Aluno;
	Object Curso;
	Object Aula;
	
	public Object getUniversidade() {
		return Universidade;
	}
	public void setUniversidade(Object universidade) {
		Universidade = universidade;
	}
	public Object getProfessor() {
		return Professor;
	}
	public void setProfessor(Object professor) {
		Professor = professor;
	}
	public Object getSala() {
		return Sala;
	}
	public void setSala(Object sala) {
		Sala = sala;
	}
	public Object getAluno() {
		return Aluno;
	}
	public void setAluno(Object aluno) {
		Aluno = aluno;
	}
	
	public Object getCurso() {
		return Curso;
	}
	public void setCurso(Object curso) {
		this.Curso = curso;
	}
	public Object getAula() {
		return Aula;
	}
	public void setAula(Object aula) {
		this.Aula = aula;
	}
}